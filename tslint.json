{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "max-classes-per-file": false,
    "no-unused-variable": false,
    "no-unused-expression": false,
    "no-console": false
  }
}
